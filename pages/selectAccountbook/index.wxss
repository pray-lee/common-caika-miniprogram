.wrapper {
    height: calc(100% - 80rpx);
    padding: 7rpx;
    overflow:hidden;
}
.list-wrapper{
    height: calc(100% - 100rpx);
    overflow:auto;
}
.title {
    color:#333;
    font-size: 28rpx;
    font-weight:600;
    line-height:40rpx;
    margin-left: 18rpx;
    margin-bottom: 4rpx;
}
.tip {
    font-size: 24rpx;
    color:#666;
    line-height: 34rpx;
    margin-left: 18rpx;
    margin-bottom: 24rpx;
}
.accountbook-wrapper{
    display:flex;
    align-items: center;
    width: 100%;
    height: 122rpx;
    background:#fff;
    border-radius: 16rpx;
    margin-bottom: 20rpx;
}
.logo-wrapper{
    width: 146rpx;
    height: 78rpx;
    background:#1c56a0;
    border-radius: 6rpx;
    display:flex;
    justify-content: center;
    align-items: center;
    margin-left: 32rpx;
}
.logo{
    width: 112rpx;
    height: 38rpx;
}
.accountbookname{
    width: 12em;
    height:40rpx;
    color:#333;
    font-weight: 400;
    font-size: 28rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow:hidden;
    margin-left: 20rpx;
}
.enter{
    width: 100rpx;
    height: 52rpx;
    text-align: center;
    line-height: 52rpx;
    background:#f0f3fc;
    color:#1c56a0;
    font-size: 26rpx;
    font-weight:400;
    border-radius: 4rpx;
    margin-left: 30rpx;
}
.no-tenant{
    display:block;
    margin:102rpx auto;
    margin-bottom: 69rpx;
    width: 390rpx;
    height: 348rpx;
}
.no-tenant-word {
    text-align: center;
    font-size: 28rpx;
    color:#333;
    line-height: 40rpx;
}
.error-mask{
    position:absolute;
    top:0;
    left:0;
    bottom:0;
    right:0;
    background:rgba(0,0,0,.5);
}
.error{
    width: 404rpx;
    height:424rpx;
    margin:0 auto;
    margin-top: 300rpx;
    background:url(data:image/png;base64,) center center no-repeat;
    background-size: 404rpx 424rpx;
}
.error{
    padding-top: 300rpx;
    box-sizing:border-box;
    text-align: center
}
.error-title{
    font-size: 24rpx;
    color:#333;
    line-height: 34rpx;
    font-weight:bold;
    margin-bottom: 10rpx;
}
.error-word{
    font-size: 20rpx;
    font-weight: 400;
    color:#666;
    line-height: 28rpx;
}
.error-close{
    margin:0 auto;
    margin-top: 38rpx;
    width: 52rpx;
    height: 52rpx;
    background:url(data:image/png;base64,) center center no-repeat;
    background-size: 52rpx 52rpx;
}