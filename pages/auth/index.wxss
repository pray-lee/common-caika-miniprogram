.mask{
    position:absolute;
    top:0;
    left:0;
    bottom:0;
    right:0;
    background:#f1f1f1;
}
.logo{
    width: 132rpx;
    height: 44rpx;
    background-size: 132rpx 44rpx;
    margin-top: 24rpx;
}
.main-wrapper{
    width: 500rpx;
    height: 524rpx;
    margin:0 auto;
    margin-top: 84rpx;
}
.logo-img, .main-img{
    width: 100%;
    height:100%;
}
.word{
    margin-top: 78rpx;
    text-align: center;
}
.title{
    font-size: 38rpx;
    font-weight:bold;
    line-height: 52rpx;
    color:#1c56a0;
}
.word-word{
    font-size: 26rpx;
    margin-top: 16rpx;
    line-height: 36rpx;
    color:#999999;
}
.button-area{
    position:relative;
    width: 100%;
    height: 90rpx;
    margin-top: 28rpx;
}
.phone-btn{
    position:absolute;
    top:0;
    left:0;
    width: 100%;
    height: 90rpx;
    background-image:url(data:image/png;base64,);
    background-position: center;
    background-repeat: no-repeat;
    background-size: 214rpx 36rpx;
    background-color:#1c56a0 !important;
    border-radius: 45rpx;
}
.not-use{
    text-align:center;
    margin-top: 64rpx;
    font-size: 26rpx;
    color:#999;
    line-height: 36rpx;
}
.checkbox-area{
    display:flex;
    justify-content: center;
    align-items: center;
    font-size:20rpx;
    color:#999;
    margin-top: 100rpx;
}
.checkbox-area .agreement-word{
    display:flex;
    flex-wrap:nowrap;
}
.checkbox-wrapper{
    position:relative;
    width: 24rpx;
    height: 24rpx;
    border-radius: 50%;
    border: 2rpx solid #1c56a0;
    margin-right: 16rpx;
}
.checkbox{
    opacity: 0;
    position:absolute;
    width: 100%;
    height: 100%;
    z-index: 9999;
}
.checked{
    position:absolute;
    left:50%;
    top:50%;
    transform: translate(-50%, -50%);
    width: 16rpx;
    height: 16rpx;
    background:#1c56a0;
    border-radius: 50%;
}
